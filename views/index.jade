extends layout

block content
  h1= title
  p Welcome to #{title}
  ul
  each user in users
    li
      span= user.name + ' ' + user.age
  
  br
  
  form(method="POST" action="/addUser")
    label Nombre 
    input(type="text" name="name")
    label Edad 
    input(type="text" name="age")
    input(type="submit" value="Enviar")
